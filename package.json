{
    "name": "OdooSnippets",
    "displayName": "Odoo Snippets",
    "description": "Snippets for Odoo developer",
    "version": "1.2.0",
    "icon": "images/icon.png",
    "publisher": "jigar-patel",
    "engines": {
        "vscode": "^1.34.0"
    },
    "galleryBanner": {
        "color": "#875A7B",
        "theme": "dark"
    },
    "license": "SEE LICENSE IN LICENSE",
    "categories": [
        "Snippets"
    ],
    "keywords": [
        "odoo", "snippets", "python", "xml", "javascript"
    ],
    "main": "extension.js",
    "activationEvents": [
        "*"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "python",
                "path": "./snippets/python_snippets.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/js_snippets.json"
            },
            {
                "language": "xml",
                "path": "./snippets/xml_snippets.json"
            },
            {
                "path": "./snippets/multilang.code-snippets"
            }
        ]
    },
    "homepage": "https://github.com/droggol/VscOdooSnippets",
    "repository": {
        "type": "git",
        "url": "https://github.com/droggol/VscOdooSnippets"
    },
    "bugs": {
        "url": "https://github.com/droggol/VscOdooSnippets/issues"
    }
}