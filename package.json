{
    "name": "OdooSnippets",
    "displayName": "Odoo Snippets",
    "description": "Snippets for Odoo developer",
    "version": "1.5.0",
    "icon": "images/icon.png",
    "publisher": "jigar-patel",
    "engines": {
        "vscode": "^1.39.0"
    },
    "galleryBanner": {
        "color": "#875A7B",
        "theme": "dark"
    },
    "license": "SEE LICENSE IN LICENSE",
    "categories": [
        "Snippets"
    ],
    "keywords": [
        "odoo",
        "snippets",
        "python",
        "xml",
        "javascript",
        "qweb",
        "csv",
        "owl"
    ],
    "main": "./dist/extension.js",
    "activationEvents": [
        "*"
    ],
    "contributes": {
        "commands": [
            {
                "command": "extension.snippetSearch",
                "title": "Odoo Snippet Search"
            }
        ],
        "snippets": [
            {
                "language": "python",
                "path": "./snippets/python-snippets.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/js-snippets.json"
            },
            {
                "language": "xml",
                "path": "./snippets/xml-snippets.json"
            },
            {
                "language": "csv",
                "path": "./snippets/csv-snippets.json"
            },
            {
                "path": "./snippets/multilang.code-snippets"
            }
        ]
    },
    "homepage": "https://github.com/droggol/VscOdooSnippets",
    "repository": {
        "type": "git",
        "url": "https://github.com/droggol/VscOdooSnippets"
    },
    "bugs": {
        "url": "https://github.com/droggol/VscOdooSnippets/issues"
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "test": "node ./dist/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.1",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.39.0",
        "@typescript-eslint/eslint-plugin": "^2.18.0",
        "@typescript-eslint/parser": "^2.18.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
        "mocha": "^7.0.1",
        "typescript": "^3.7.5",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {
        "latest-version": "^7.0.0"
    }
}
